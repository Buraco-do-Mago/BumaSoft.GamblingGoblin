<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <TargetFramework>net10.0</TargetFramework>
    <PackageId>BumaSoft.GamblingGoblin</PackageId>
    <Version>1.0.0</Version>
    <Authors>Lucas Renato</Authors>
    <Company>BumaSoft</Company>
    <Description>A library to simulate dice rolls and statistics.</Description>
    <PackageTags>rng;dice;rpg;statistics;luck;roll;dice-roll</PackageTags>
    <PackageProjectUrl>https://github.com/Buraco-do-Mago/BumaSoft.GamblingGoblin</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Buraco-do-Mago/BumaSoft.GamblingGoblin</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
